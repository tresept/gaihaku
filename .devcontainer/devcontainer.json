{
  "name": "gaihakudev",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "go-app",
  "workspaceFolder": "/go/src",
  "forwardPorts": [5432],
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/git-auth:1": {
      "gitCredentialManager": "true"
    }
  },
  "remoteUser": "vscode",
  "mounts": [
    "source=${localEnv:HOME}/.ssh,target=/home/vscode/.ssh,type=bind,consistency=cached"
  ],
  "containerEnv": {
    "SESSION_SECRET_KEY": "your-secret-key-here"
  }
}